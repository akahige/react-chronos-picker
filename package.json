{
  "name": "@asidd/react-chronos-picker",
  "version": "1.1.11",
  "description": "A customizable, flexible and easy to use date picker library for React.",
  "author": {
    "name": "Akahige"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akahige/react-chronos-picker.git"
  },
  "bugs": {
    "url": "https://github.com/akahige/react-chronos-picker/issues"
  },
  "homepage": "https://github.com/akahige/react-chronos-picker#readme",
  "files": [
    "dist"
  ],
  "main": "./dist/ReactChronosPicker.umd.js",
  "module": "./dist/ReactChronosPicker.es.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/index.css",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/ReactChronosPicker.es.js",
      "require": "./dist/ReactChronosPicker.umd.js",
      "types": "./dist/types/index.d.ts",
      "style": "./dist/index.css"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "./*": [
        "./dist/*"
      ]
    }
  },
  "type": "module",
  "keywords": [
    "react",
    "date picker",
    "react component",
    "calendar",
    "date"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build && tsc --emitDeclarationOnly",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@asidd/chronos": "^1.1.2",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "@asidd/chronos": "^1.1.2",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.27",
    "@storybook/addon-interactions": "^7.0.27",
    "@storybook/addon-links": "^7.0.27",
    "@storybook/blocks": "^7.0.27",
    "@storybook/react": "^7.0.27",
    "@storybook/react-vite": "^7.0.27",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "prop-types": "^15.8.1",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.27",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^1.0.5",
    "vite-plugin-css-injected-by-js": "^3.2.1"
  }
}
